{
  "manifest_version": 3,
  "name": "Smart Blocker",
  "description": "Blocks sites containing specific words from storage",
  "version": "4.0",
  "permissions": ["storage", "scripting", "webNavigation"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html"
  
  }
}
